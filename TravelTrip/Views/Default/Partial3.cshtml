@model List<TravelTrip.Models.Classes.Blog>

<div class="padding">
    <h4>En Son 10 Blog</h4>
    
    <ul class="list list-pad">
        @{ int i =0;}
        @foreach (var item in Model)
        {
            <li class="list-bg"><span>@{i++;}@i</span><a href="#">@item.Title</a></li>
        }
        </ul>
    
    <a href="/Blog/Index/" class="link1">Tüm Blogları Gör</a>
</div>
