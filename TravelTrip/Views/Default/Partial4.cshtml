@model List<TravelTrip.Models.Classes.Blog>


<div class="padding1">
    <h4>Our Best Places</h4>
    <div class="box-left">
        @foreach (var item in Model)
        {
            <div class="bot-img">
                <img src="@item.ImageUrl"style="width:120px; height:65px;" alt="" />
            </div>
            <div class="bot-desc">
                <p>@item.Description.Substring(0, 75)..."</p>
            </div>
            <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial5", "Default")
</div>
